<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo3"
    tools:context=".Registro">

    <Button
        android:id="@+id/btn_registrarse_registro"
        android:layout_width="144dp"
        android:layout_height="41dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/bordes_redondos"
        android:fontFamily="monospace"
        android:text="@string/btn_reg"
        android:textColor="#100F0F"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_atras"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:targetApi="lollipop" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="170dp"
        android:layout_height="166dp"
        android:layout_marginBottom="12dp"
        android:contentDescription="@string/logo"
        app:layout_constraintBottom_toTopOf="@+id/et_user_registro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/logo_001"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_atras"
        android:layout_width="144dp"
        android:layout_height="41dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/bordes_redondos"
        android:fontFamily="monospace"
        android:text="@string/btn_atras"
        android:textColor="#0B0A0A"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:targetApi="lollipop" />

    <EditText
        android:id="@+id/et_user_registro"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bordes_redondos_edti"
        android:drawableLeft="@mipmap/ic_launcher_user"
        android:ems="10"
        android:hint="@string/userName"
        android:importantForAutofill="no"
        android:textColor="@color/black"
        android:inputType="textPersonName"
        android:textColorHint="@color/cardview_dark_background"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/et_gmail_registro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et_gmail_registro"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bordes_redondos_edti"
        android:drawableLeft="@mipmap/ic_launcher_correo"
        android:ems="10"
        android:hint="@string/txt_email"
        android:importantForAutofill="no"
        android:inputType="textEmailAddress"
        android:textColor="#000000"
        android:textColorHint="@color/cardview_dark_background"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/et_telefono_registro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et_telefono_registro"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bordes_redondos_edti"
        android:drawableLeft="@mipmap/ic_launcher_telefono"
        android:ems="10"
        android:hint="@string/txt_numero"
        android:importantForAutofill="no"
        android:inputType="phone"
        android:textColor="#000000"
        android:textColorHint="@color/cardview_dark_background"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/et_contraseña_registro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et_contraseña_registro"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bordes_redondos_edti"
        android:drawableLeft="@mipmap/ic_launcher_clave"
        android:ems="10"
        android:hint="@string/txt_password"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        android:textColor="#000000"
        android:textColorHint="@color/cardview_dark_background"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/et_r_password_registro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et_r_password_registro"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/bordes_redondos_edti"
        android:drawableLeft="@mipmap/ic_launcher_clave"
        android:ems="10"
        android:hint="@string/txt_repetir_password"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        android:textColor="#000000"
        android:textColorHint="@color/cardview_dark_background"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_registrarse_registro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>